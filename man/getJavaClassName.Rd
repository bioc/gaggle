\name{getJavaClassName}
\alias{getJavaClassName}
\title{Get Java class name}
\description{Get the class name of a Java object for debugging purposes. Used with RJava objects.}

\usage{getJavaClassName(jobj, quiet=FALSE)}

\arguments{
  \item{jobj}{a Java object}
  \item{quiet}{complain if jobj isn't a java object}
}

\value{Returns a string.}

\keyword{Java}
